{
  "name": "chrome_demo",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "tabs"
  ],
  "background": {
  "scripts": ["background.js"],
  "persistent": false
   },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_title": "网址收藏",
    "default_icon": "images/32.png"
  },
  "incognito": "split",
  "manifest_version": 2  // 注意这个必须写2
}
